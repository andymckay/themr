{
  "description": "Themr",
  "homepage_url": "https://github.com/johngruen/themr",
  "manifest_version": 2,
  "name": "Themr",
  "permissions": [
    "theme",
    "storage"
  ],
  "browser_action": {
    "browser_style": true,
    "default_title": "Themr",
    "default_icon": {
        "32": "icon.svg"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48":"icon.svg",
    "96": "icon.svg"
  },
  "version": "0.0.2",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  }
}
